{
  "name": "mmv",
  "version": "1.0.0",
  "private": true,
  "description": "Matty's Mobile Valeting",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "cypress:run": "cypress run --env endpoint=http://localhost:9000 --browser chrome --headless",
    "cypress:open": "cypress open --env endpoint=http://localhost:9000",
    "cypress:ci": "npx start-server-and-test serve http://localhost:9000 cypress:run",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lighthouse": "lighthouse https://www.valet.expert --output-path='./lighthouse.html' --view"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@fontsource/dm-sans": "^4.5.0",
    "@fontsource/oswald": "^4.5.0",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@types/jest": "^26.0.24",
    "babel-jest": "^27.0.6",
    "babel-preset-gatsby": "^1.9.0",
    "gatsby": "^3.9.1",
    "gatsby-image": "^3.9.0",
    "gatsby-plugin-google-analytics": "^3.9.0",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-robots-txt": "^1.6.8",
    "gatsby-plugin-sass": "^4.9.0",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-sitemap": "^4.5.0",
    "gatsby-remark-images-contentful": "^4.6.0",
    "gatsby-source-contentful": "^5.9.0",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-sharp": "^3.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.2"
  },
  "devDependencies": {
    "cypress": "^7.7.0",
    "cypress-audit": "^1.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lighthouse": "^8.0.0",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.12.6"
  }
}
