{
  "name": "mmv",
  "version": "1.0.0",
  "private": true,
  "description": "Matty's Mobile Valeting",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "cypress:run": "percy exec -- cypress run --browser chrome --headless",
    "cypress:open": "CYPRESS_BASE_URL=http://localhost:8000 cypress open",
    "cypress:ci": "npx start-server-and-test serve http://localhost:9000 cypress:run",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "npm run lint && npm run cypress:ci",
    "lighthouse": "lighthouse https://www.valet.expert --output-path='./lighthouse.html' --view"
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@fontsource/dm-sans": "^4.5.0",
    "@fontsource/oswald": "^4.5.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "babel-preset-gatsby": "^2.3.0",
    "gatsby": "^3.14.3",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-image": "^1.14.1",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-robots-txt": "^1.6.13",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.1",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-test-renderer": "^17.0.2"
  },
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.70",
    "@percy/cypress": "^3.1.1",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.3.1",
    "cypress": "^8.6.0",
    "cypress-audit": "^1.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lighthouse": "^8.5.1",
    "prettier": "^2.4.1",
    "sass": "^1.43.3",
    "start-server-and-test": "^1.14.0"
  }
}
